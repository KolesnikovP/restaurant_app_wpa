{
  "version": 2,
  "routes": [
    { "src": "/sw.js", "dest": "/sw.js" },
    { "src": "/manifest.webmanifest", "dest": "/manifest.webmanifest" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/favicon.ico", "dest": "/favicon.ico" },
    { "src": "/(.*\\.(?:js|css|png|jpg|jpeg|gif|svg|webp|ico|txt|map|json))$", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
